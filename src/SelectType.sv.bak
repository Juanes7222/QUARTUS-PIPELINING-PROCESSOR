module SelectType(
	input [6:0] opcode,
	output [7:0] ascii
	);
	
	always @* begin
		case (opcode)
			7'b0110011: {4b0,"R"};
			7'b0010011: {4b0,"I"};
			7'b0000011: {"I","L"};
			7'b0100011: {4b0,"S"};
			7'b1100011: {4b0,"B"};
			7'b1101111: {4b0,"J"};
			7'b1100111: {"I","S"};
			7'b0110111: {4b0,"U"};
			default: "X";
		endcase
	end